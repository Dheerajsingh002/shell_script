echo "enter the number"
read n
if [ `expr $n` == 1 ]
then
	echo "sunday"
fi
if [ `expr $n` == 2 ]
then
	echo "monday"
fi
if [ `expr $n` == 3 ]
then
        echo "tuesday"
fi
if [ `expr $n` == 4 ]
then
        echo "wednesday"
fi
if [ `expr $n` == 5 ]
then
        echo "thursday"
fi
if [ `expr $n` == 6 ]
then
        echo "friday"
fi
if [ `expr $n` == 7 ]
then
        echo "saturday"
fi
